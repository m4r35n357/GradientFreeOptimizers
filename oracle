#!/bin/bash

model=$1
dim=$2
lower=$3
upper=$4

. shell-functions

size=$(echo "scale=3; ($upper - $lower) / 10.0;" | /usr/bin/bc)

if [ $dim -ge 8 ]; then
    adaptive=1
else
    adaptive=0
fi

./nm-$model-std 6 0 1.0e-6 10000 $size $adaptive $(random $dim $lower $upper) >/dev/null

./whale-$model-std 6 0 $dim 200 500 $lower $upper >/dev/null

./spiral-$model-std 6 0 $dim 200 500 0.05 $lower $upper >/dev/null
